      "termDefinitions" : {
        "en" : {
          "at0.1" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1000",
            "text" : "Standing",
            "description" : "Standing at the time of blood pressure measurement."
          },
          "at0.2" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1001",
            "text" : "Sitting",
            "description" : "Sitting (for example on bed or chair) at the time of blood pressure measurement."
          },
          "at0.3" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1002",
            "text" : "Reclining",
            "description" : "Reclining at the time of blood pressure measurement."
          },
          "at0.4" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1003",
            "text" : "Lying",
            "description" : "Lying flat at the time of blood pressure measurement."
          },
          "at0.5" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1014",
            "text" : "Lying with tilt to left",
            "description" : "Lying flat with some lateral tilt, usually angled towards the left side.   Commonly required in the last trimester of pregnancy to relieve aortocaval compression."
          },
          "at0.6" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1044",
            "text" : "Awake",
            "description" : "The individual is fully conscious."
          },
          "at0.7" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1045",
            "text" : "Sleeping",
            "description" : "The individual is in the natural state of bodily rest."
          }
        },
        "ja" : {
          "at0.1" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1000",
            "text" : "立位",
            "description" : "血圧測定時に立位"
          },
          "at0.2" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1001",
            "text" : "座位",
            "description" : "血圧測定時に（たとえば、ベッドやいすの上で）坐位"
          },
          "at0.3" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1002",
            "text" : "斜位",
            "description" : "血圧測定時に斜位"
          },
          "at0.4" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1003",
            "text" : "臥位",
            "description" : "血圧測定時に臥位"
          },
          "at0.5" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1014",
            "text" : "左側臥位",
            "description" : "通常は頭から足までを水平にしてから左側に傾けられた状態。一般的には妊娠の第3トリメスターで大動脈・静脈を圧排から解放するために求められる体位である。"
          },
          "at0.6" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1044",
            "text" : "覚醒",
            "description" : "対象は完全に意識がある。"
          },
          "at0.7" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1045",
            "text" : "睡眠中",
            "description" : "対象は自然な休眠状態にある"
          }
        },
        "de" : {
          "at0.1" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1000",
            "text" : "Stehend",
            "description" : "Stehend zum Zeitpunkt der Blutdruckmessung."
          },
          "at0.2" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1001",
            "text" : "Sitzend",
            "description" : "Sitzend zum Zeitpunkt der Blutdruckmessung (z.B. auf einem Bett oder Stuhl)."
          },
          "at0.3" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1002",
            "text" : "Zurückgelehnt",
            "description" : "Patient zurückgelehnt zum Zeitpunkt der Blutdruckmessung."
          },
          "at0.4" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1003",
            "text" : "Liegend",
            "description" : "Flach liegend zum Zeitpunkt der Blutdruckmessung."
          },
          "at0.5" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1014",
            "text" : "Nach links geneigt liegend",
            "description" : "Flach liegend mit seitlicher Neigung, normalerweise zur linken Seite. Häufig verwendet im letzten Drittel eine Schwangerschaft, um aorto-cavale Kompression zu vermeiden."
          },
          "at0.6" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1044",
            "text" : "Aufmerksam und wach",
            "description" : "Die untersuchte Person ist bei vollem Bewusstsein."
          },
          "at0.7" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1045",
            "text" : "Schlafend",
            "description" : "Die untersuchte Person schläft."
          }
        },
        "zh-cn" : {
          "at0.1" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1000",
            "text" : "立位",
            "description" : "测量血压时身体处于站立体位"
          },
          "at0.2" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1001",
            "text" : "坐位",
            "description" : "测量血压时身体处于坐位，又称为“坐姿”。"
          },
          "at0.3" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1002",
            "text" : "侧卧位",
            "description" : "测量血压时身体处于45度角侧卧位或者说采取的是斜靠的姿势。"
          },
          "at0.4" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1003",
            "text" : "平卧位",
            "description" : "测量血压时受检者采取的是平躺或者说平卧的姿势，又称为“仰卧位”。"
          },
          "at0.5" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1014",
            "text" : "左斜卧位",
            "description" : "测量血压时受检者采取的是平躺或者说平卧姿势，且有向侧位一定程度的倾斜，通常是斜向左侧。在妊娠晚期，为了缓解主腔静脉压迫（aortocaval compression），常常需要采取这种体位。"
          },
          "at0.6" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1044",
            "text" : "警觉且清醒",
            "description" : "受检对象意识完全清楚。"
          },
          "at0.7" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1045",
            "text" : "睡眠状态",
            "description" : "受检对象处在全身休息的自然状态下。"
          }
        },
        "nl" : {
          "at0.1" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1000",
            "text" : "Staand",
            "description" : "Bloeddrukmeting bij staand individu"
          },
          "at0.2" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1001",
            "text" : "Zittend",
            "description" : "Bloeddrukmeting bij zittend (b.v. op bed of in stoel) individu"
          },
          "at0.3" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1002",
            "text" : "Halfzittend",
            "description" : "Halfzittend op het moment van de bloeddrukmeting"
          },
          "at0.4" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1003",
            "text" : "Liggend",
            "description" : "Platliggend op het moment van de bloeddrukmeting"
          },
          "at0.5" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1014",
            "text" : "Liggend met kanteling naar linkerzijde",
            "description" : "Platliggend met enige laterale kanteling, meestal gekanteld naar de linkerzijde. Gebruikelijk benodigd in het laatste trimester van de zwangerschap om aortacavale compressie te verlichten."
          },
          "at0.6" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1044",
            "text" : "Alert en wakker",
            "description" : "Individu is volledig bij bewustzijn"
          },
          "at0.7" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1045",
            "text" : "Slapend",
            "description" : "Individu is in de natuurlijke slaap."
          }
        },
        "ru" : {
          "at0.1" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1000",
            "text" : "Стоя",
            "description" : "Пациент стоит во время измерения АД."
          },
          "at0.2" : {
            "@type" : "ARCHETYPE_TERM",
            "code" : "at1001",
            "text" : "Сидя",
            "description" : "Пациент сидит во время измерения АД (на стуле, кровати или кресле)."
          },
          "at0.3" : {
            "@type" : "ARCHETYPE_TERM",
